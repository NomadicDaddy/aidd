{
	"arrowParens": "always",
	"attributeSort": "ASC",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"jsonRecursiveSort": true,
	"jsxSingleQuote": false,
	"plugins": [
		"prettier-plugin-organize-attributes",
		"prettier-plugin-sort-json",
		"prettier-plugin-tailwindcss"
	],
	"printWidth": 100,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true
}
